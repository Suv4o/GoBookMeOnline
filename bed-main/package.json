{
  "name": "bed-main",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=prod nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "start:stage": "cross-env NODE_ENV=stage nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "version--patch": "npm version patch && tsc versioning.ts && node versioning.js",
    "version--minor": "npm version minor && tsc versioning.ts && node versioning.js",
    "version--major": "npm version major && tsc versioning.ts && node versioning.js",
    "typeorm": "cross-env NODE_ENV=dev typeorm-ts-node-esm -d ormconfig.ts",
    "typeorm:test": "cross-env NODE_ENV=test typeorm-ts-node-esm -d ormconfig.ts",
    "schema:create": "cross-env NODE_ENV=dev ts-node fixtures/database.ts create",
    "schema:create:test": "cross-env NODE_ENV=test ts-node fixtures/database.ts create",
    "schema:drop": "cross-env NODE_ENV=dev ts-node fixtures/database.ts drop",
    "schema:drop:test": "cross-env NODE_ENV=test ts-node fixtures/database.ts drop",
    "delete:table:user": "cross-env NODE_ENV=dev ts-node fixtures/database.ts delete:table:user",
    "delete:table:user:test": "cross-env NODE_ENV=test ts-node fixtures/database.ts delete:table:user",
    "firebase:drop": "cross-env NODE_ENV=dev ts-node fixtures/firebase.ts drop",
    "firebase:drop:test": "cross-env NODE_ENV=test ts-node fixtures/firebase.ts drop"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/pg": "^8.6.5",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.1.0",
    "handlebars": "^4.7.7",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@types/express": "^4.17.14",
    "@types/jest": "29.1.2",
    "@types/node": "^18.8.4",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.1.2",
    "prettier": "^2.7.1",
    "simple-git": "^3.14.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.0",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "types-package-json": "^2.0.39",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
