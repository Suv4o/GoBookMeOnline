{
  "name": "fed-main",
  "private": true,
  "version": "0.2.0",
  "engines": {
    "node": ">=v18.0.0"
  },
  "scripts": {
    "dev": "vite --https",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --environment jsdom",
    "test:unit:coverage": "cross-env NODE_ENV=test vitest --coverage --environment jsdom",
    "test:unit": "cross-env NODE_ENV=test vitest run --environment jsdom",
    "version--patch": "npm version patch && tsc versioning.ts && node versioning.js",
    "version--minor": "npm version minor && tsc versioning.ts && node versioning.js",
    "version--major": "npm version major && tsc versioning.ts && node versioning.js",
    "database:table:delete": "cross-env NODE_ENV=test ts-node src/config/database.utils.ts delete"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.6",
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@vueuse/core": "^8.9.1",
    "dotenv": "^16.0.1",
    "firebase": "^9.9.0",
    "pg": "^8.8.0",
    "pinia": "^2.0.14",
    "simple-git": "^3.10.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.13",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.0.3",
    "@types/pg": "^8.6.5",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.5",
    "ts-node": "^10.9.1",
    "types-package-json": "^2.0.39",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-eslint": "^1.6.1",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.38.3"
  }
}
