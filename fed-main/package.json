{
  "name": "fed-main",
  "private": true,
  "version": "0.2.0",
  "engines": {
    "node": ">=v18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --environment jsdom",
    "test:unit:coverage": "cross-env NODE_ENV=test vitest --coverage --environment jsdom",
    "test:unit": "cross-env NODE_ENV=test vitest run --environment jsdom",
    "version--patch": "npm version patch && tsc versioning.ts && node versioning.js",
    "version--minor": "npm version minor && tsc versioning.ts && node versioning.js",
    "version--major": "npm version major && tsc versioning.ts && node versioning.js",
    "database:table:delete": "cross-env NODE_ENV=test ts-node src/config/cli/database.utils.ts delete",
    "firebase:drop": "cross-env NODE_ENV=test ts-node src/config/cli/firebase.ts drop",
    "database:firebase:drop": "npm run database:table:delete && npm run firebase:drop"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@heroicons/vue": "^2.0.12",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@vueuse/core": "^9.3.0",
    "dotenv": "^16.0.3",
    "firebase": "^9.11.0",
    "firebase-admin": "^11.1.0",
    "pg": "^8.8.0",
    "pinia": "^2.0.23",
    "simple-git": "^3.14.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.8.4",
    "@types/pg": "^8.6.5",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "types-package-json": "^2.0.39",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.24.1",
    "vue-tsc": "^1.0.3"
  }
}
